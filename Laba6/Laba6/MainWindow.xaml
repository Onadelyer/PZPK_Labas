<Window x:Class="Laba6.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Laba6"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="#FFDFDFDF">
        <Frame Name="MainFrame" Margin="0,50,0,0" Background="White"/>
        <StackPanel Orientation="Horizontal" Margin="0,0,0,389">
            <Button Content="Task1" Width="60" Margin="0,0,20,0" Click="ButtonClick1"/>
            <Button Content="Task2" Width="60" Margin="0,0,20,0" Click="ButtonClick2"/>
            <Button Content="Task3" Width="60" Margin="0,0,20,0" Click="ButtonClick3"/>
            <Button Content="Task4" Width="60" Margin="0,0,20,0" Click="ButtonClick4"/>
            <Button Content="Task5" Width="60" Margin="0,0,20,0" Click="ButtonClick5"/>
            <Button Content="Task6" Width="60" Margin="0,0,20,0" Click="ButtonClick6"/>
            <Button Content="Task7" Width="60" Margin="0,0,20,0" Click="ButtonClick7"/>
            <Button Content="Task8" Width="60" Margin="0,0,20,0" Click="ButtonClick8"/>
        </StackPanel>

    </Grid>
</Window>
