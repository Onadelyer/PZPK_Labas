<Window x:Class="Laba3_Csharp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Laba3_Csharp"
        mc:Ignorable="d"
        Title="Laba3" Height="450" Width="800" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" VerticalAlignment="Center">
    <Grid Background="#FF6C6C6C">
        <Frame x:Name="MainFrame" Margin="0,57,0,0" Background="White"/>
        <StackPanel HorizontalAlignment="Left" Orientation="Horizontal" VerticalAlignment="Top">
            <Button Content="Task 1" Height="57" Width="103" Click="Task1ButtonClick" FontSize="22" FontFamily="Calibri"/>
            <Button Content="Task 2" Height="57" Width="103" Click="Task2ButtonClick" FontSize="22" FontFamily="Calibri"/>
            <Button Content="Task 3" Height="57" Width="103" Click="Task3ButtonClick" FontSize="22" FontFamily="Calibri"/>
            <Button Content="Task 4" Height="57" Width="103" Click="Task4ButtonClick" FontSize="22" FontFamily="Calibri"/>

        </StackPanel>

    </Grid>
</Window>
