<Page x:Class="Laba10.Tasks.Task2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Laba10.Tasks"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Task2">

    <Grid>
        <StackPanel Background="White">
            <Button Content="Push Front" Click="PushFront_Click"/>
            <Button Content="Push Back" Click="PushBack_Click"/>
            <Button Content="Pop Front" Click="PopFront_Click"/>
            <Button Content="Pop Back" Click="PopBack_Click"/>
            <Button Content="Front" Click="Front_Click"/>
            <Button Content="Back" Click="Back_Click"/>
            <Button Content="Size" Click="Size_Click"/>
            <Button Content="Clear" Click="Clear_Click"/>
            <ListBox x:Name="dequeListBox" Width="200" Height="200"/>
        </StackPanel>
    </Grid>

</Page>
